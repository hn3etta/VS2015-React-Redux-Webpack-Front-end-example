    "express": "4.14.0",
    "gsap": "1.19.0",
    "immutable": "3.8.1",
    "jwt-decode": "2.1.0",
    "moment": "2.17.1",
    "react": "15.4.1",
    "react-chartjs-2": "1.6.2",
    "react-d3-components": "0.6.5",
    "react-dom": "15.4.1",
    "react-modal-dialog": "4.0.4",
    "react-redux": "5.0.1",
    "react-router": "3.0.0",
    "react-router-redux": "4.0.7",
    "react-stickynode": "1.2.1",
    "redux": "3.6.0",
    "redux-persist": "4.0.1",
    "redux-persist-transform-encrypt": "1.0.1",
    "redux-persist-transform-immutable": "4.1.1",
    "redux-thunk": "2.1.0",
    "whatwg-fetch": "2.0.1"
  },
