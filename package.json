    "immutable": "^3.8.1",
    "jquery": "2.2.3",
    "jwt-decode": "^2.1.0",
    "moment": "^2.14.1",
    "node-sass": "^3.8.0",
    "react": "15.0.2",
    "react-addons-css-transition-group": "^15.2.0",
    "react-addons-pure-render-mixin": "^15.3.0",
    "react-chartjs-2": "^1.5.0",
    "react-d3-components": "^0.6.4",
    "react-dom": "15.0.2",
    "react-modal-dialog": "^3.0.1",
    "react-motion": "^0.4.4",
    "react-redux": "4.4.5",
    "react-router": "2.4.0",
    "react-router-redux": "4.0.4",
    "react-stickynode": "^1.1.5",
    "redux": "3.5.2",
    "redux-persist": "^3.2.2",
    "redux-persist-transform-encrypt": "^0.1.2",
    "redux-persist-transform-immutable": "^3.1.0",
